@model IEnumerable<Evaluations.Models.COURSE>

@{ 
    ViewBag.Title = "Courses Available to Survey"; 
} 

@*<link href="/Content/PagedList.css" rel="stylesheet" type="text/css" />*@

@*@using PagedList.Mvc;
@using PagedList;*@ 

 <script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.custom.min.js")" type="text/javascript"></script>

<h2>This list of courses are either set to complete or 1 days past the activities last end date. </h2>

<table>
    <tr>
    <tr>
        <th>
            <strong> Select a course </strong>
        </th>
        <th>
            <strong> Activity ID </strong>
        </th>
        <th>
            <strong> Activity Title - Course Title </strong>
        </th>   
        <th>
            <strong> Supervisor Name </strong>
        </th>
        <th>
            <strong> Barcode </strong>
        </th>

        <th>
            <strong> Start Date & Time </strong>
        </th>
        <th>
            <strong> End Date & Time  </strong>
        </th>
        <th>
            <strong> Course Status </strong>
        </th>
        <th>
            <strong> Course Survey Status </strong>
        </th>  
        <th>
            <strong> Camp Flag </strong>
        </th>               
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ACTIVITY.activity_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.title)
        </td>
        <td>
            
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.barcode_number)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.last_start_datetime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.last_end_datetime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.course_status_id)
        </td>
        <td>
            
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.camp_flag)
        </td>
    </tr>
}

</table>
