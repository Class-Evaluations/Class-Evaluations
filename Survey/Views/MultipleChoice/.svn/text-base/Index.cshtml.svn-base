@model IEnumerable<Survey.Models.QUESTION_MULTIPLE_CHOICE>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            QUESTION
        </th>
        <th>
            choice_text
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.QUESTION.question_text)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.choice_text)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.question_multiple_choice_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.question_multiple_choice_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.question_multiple_choice_id })
        </td>
    </tr>
}

</table>
