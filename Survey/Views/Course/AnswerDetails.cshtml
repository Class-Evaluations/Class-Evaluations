@model IEnumerable<Survey.ViewModels.ReportData>
@{
    ViewBag.Title = "Answer Details";
}

<script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.custom.min.js")" type="text/javascript"></script>

<h2>Answer Details</h2>
<h3>This is just an example of a quick view for the answers, this view only displays what was answered for the Scale questions.</h3>

<table>
    <tr>
        <th>
            Question ID
        </th>
        <th>
            Question
        </th>
        <th>
            Responses
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>

        <td>
            @item.questionID
        </td>
        <td>
            @item.questionText
        </td>
        <td>
            @item.responseStat
        </td>
    </tr>
}

</table>
